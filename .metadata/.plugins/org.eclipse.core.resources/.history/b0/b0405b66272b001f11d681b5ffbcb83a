package aplication;

import java.util.ArrayList;
import java.util.List;

public class Program {

	public static void main(String[] args) {
		
		List<String> list = new ArrayList<>();
		
		list.add("Fritz");
		list.add("Hermann");
		list.add("Sch√ºller");
		list.add(2, "Fulano");
		System.out.println(list.size());
		list.remove("Hermann");
		list.remove(0);
		for (String obj : list) {
			System.out.println(obj);
		}
		System.out.println(list.size());

	}

}
